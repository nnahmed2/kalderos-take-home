{
  "compilerOptions": {
    "target": "es2016",
    "lib": ["dom", "esnext"],
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true
  },
  "include": ["src"]
}